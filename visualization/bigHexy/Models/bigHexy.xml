<?xml version="1.0"?>

<PropertyList>

  <!-- main hex frame -->
  <path>bigHexy.ac</path>

  <!-- submodels -->
  <model>
    <path>rotors/40in_prop_CW.ac</path>
    <offsets>
      <x-m>0.0</x-m>
      <y-m>2</y-m>
      <z-m>0.4</z-m>
    </offsets>
    <orientation>
      <heading-deg>0</heading-deg>
    </orientation>
  </model>

  <model>
    <path>rotors/40in_prop_CW.ac</path>
    <offsets>
      <x-m>0.0</x-m>
      <y-m>-2</y-m>
      <z-m>0.4</z-m>
    </offsets>
    <orientation>
      <heading-deg>0</heading-deg>
    </orientation>
  </model>

 <model>
    <path>rotors/40in_prop_R90_CW.ac</path>
