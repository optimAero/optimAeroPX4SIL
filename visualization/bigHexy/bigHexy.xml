                    <onset-redout-sec type="double">45</onset-redout-sec>
                    <fast-redout-sec type="double">3.5</fast-redout-sec>
                    <recover-fast-sec type="double">7</recover-fast-sec>
                    <recover-slow-sec type="double">15</recover-slow-sec>
                </parameters>
