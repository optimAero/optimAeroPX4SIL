      <x-m>1.732</x-m>
      <y-m>1.0</y-m>
      <z-m>0.4</z-m>
    </offsets>
    <orientation>
      <heading-deg>0</heading-deg>
    </orientation>
  </model>

  <!-- Rear-right -->
