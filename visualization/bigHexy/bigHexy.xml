            <path>/f16/avionics/ils-volume</path>
            <path>/f16/ews/rwr-volume</path>
            <path>/f16/avionics/intercom-volume</path>
            <path>/f16/effects/dirt</path>
            <path>/f16/effects/rust</path>
            <path>/environment/season</path>
